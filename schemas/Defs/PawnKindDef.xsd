        </xs:annotation>
      </xs:element>
      <xs:element name="styleItemTags">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="li" type="StyleItemTagWeightedType" minOccurs="0"
              maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="forcedHair" type="DefNameType" />
      <xs:element name="missingParts">

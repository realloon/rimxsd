      <xs:element name="burnableByRecipe" type="xs:boolean" minOccurs="0" />
      <xs:element name="randomizeRotationOnSpawn" type="xs:boolean" minOccurs="0" />
      <xs:element name="damageMultipliers" minOccurs="0">
        <xs:complexType>
          <xs:choice maxOccurs="unbounded">
            <xs:element name="li" type="DamageMultiplierType" minOccurs="0"
              maxOccurs="unbounded" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="isTechHediff" type="xs:boolean" minOccurs="0" />
      <xs:element name="recipeMaker" type="RecipeMakerPropertiesType" minOccurs="0" />
      <xs:element name="minifiedDef" type="xs:string" minOccurs="0" />
      <xs:element name="isUnfinishedThing" type="xs:boolean" minOccurs="0" />
      <xs:element name="leaveResourcesWhenKilled" type="xs:boolean" minOccurs="0" />
      <xs:element name="slagDef" type="xs:string" minOccurs="0" />
      <xs:element name="isFrameInt" type="xs:boolean" minOccurs="0" />
      <xs:element name="multipleInteractionCellOffsets" minOccurs="0">
